{
    "name": "laercionunesc/captcha-cf",
    "description": "Easily use Turnstile Cloudflare plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "CaptchaCf\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CaptchaCf\\Test\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
